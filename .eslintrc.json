{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "eqeqeq": "error",
    "camelcase": "warn",
    "quotes": ["off", "double", { "allowTemplateLiterals": true }],
    "semi": "off",
    "no-proto": "warn"
  }
}
